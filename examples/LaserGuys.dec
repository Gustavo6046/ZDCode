Actor LaserGuyY : ShotgunGuy
{
    DropItem LaserGun
    DropItem "BaseLaserModule__deriv_433c301996528d61021cc650e7fd79343411ab95", 40
    Translation "80:111=%[0.00,0.00,0.00]:[1.99,1.51,0.00]", "5:8=%[0.00,0.00,0.00]:[1.61,0.94,0.04]"
    States {
        Missile:
            goto Aim
        
        Aim:
            SPOS E 8 A_FaceTarget(20)
            TNT1 A 0 A_CheckLOF(2)
