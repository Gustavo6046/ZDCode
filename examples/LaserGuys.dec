            goto Shoot
            TNT1 A 0
            SPOS E 2 A_FaceTarget(5)
            TNT1 A 0 A_Jump(256-(256*(30)/100), 21)
                TNT1 A 0 A_JumpIfTargetInLOS(19)
                    SPOS E 2
                        TNT1 A 0 A_JumpIfTargetInLOS(2)
                        TNT1 A 0 A_Jump(256, 2)
                        goto Missile
                        TNT1 A 0
                    SPOS E 2
                        TNT1 A 0 A_JumpIfTargetInLOS(2)
                        TNT1 A 0 A_Jump(256, 2)
                        goto Missile
                        TNT1 A 0
                    SPOS E 2
                        TNT1 A 0 A_JumpIfTargetInLOS(2)
                        TNT1 A 0 A_Jump(256, 2)
                        goto Missile
                        TNT1 A 0
                    SPOS E 2
                        TNT1 A 0 A_JumpIfTargetInLOS(2)
                        TNT1 A 0 A_Jump(256, 2)
                        goto Missile
                        TNT1 A 0
                    SPOS C 3
                    goto See
                TNT1 A 0 A_Jump(256, 1)
        
                TNT1 A 0
            TNT1 A 0
        
        Shoot:
            SPOS F 0 A_PlaySound("weapons/railgf", CHAN_WEAPON)
            SPOS F 6 A_CustomMissile("DoomLaserY", 48)
            SPOS F 2
            SPOS E 3
            goto See
    }
}


