Actor RewardChest
{
    Scale 0.5
    
    +AMBUSH
    +QUICKTORETALIATE
    +LOOKALLAROUND
    States {
        Spawn:
            ELEC A 1
            goto Look
        
        Look:
            ELEC A 1 A_LookEx(LOF_NOSOUNDCHECK)
            loop
        
        See:
            ELEC A 1 A_JumpIfCloser(80, "Reward")
            goto Spawn
        
