Actor RewardFX
{
    Alpha 0.5
    Gravity 0
    RenderStyle Translucent
    Scale 1.2
    States {
        Spawn:
            MISL C 9
            MISL D 9
            MISL E 9
            stop
    }
}


Actor RewardFXBright : RewardFX
{

    States {
        Spawn:
            MISL C 9  Bright
            MISL D 9  Bright
            MISL E 9  Bright
            stop
    }
}


